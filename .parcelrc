{
  "extends": "@parcel/config-default",
  "resolvers": ["@parcel/resolver-glob", "parcel-resolver-ignore", "..."],
  "transformers": {
    "*.pug": ["@parcel/transformer-pug", "..."]
  },
  "optimizers": {
    "*.css": ["@parcel/optimizer-css", "..."],
    "*.html": ["@parcel/optimizer-htmlnano", "..."],
    "*.{jpg,jpeg,png}": ["@parcel/optimizer-image", "..."]
  }
}